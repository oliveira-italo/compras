type Cliente {
    id:ID!
    nome:String!
    email:String!
    compras:[Compra]
    comprasByQtd(qtd:Int!):[Compra]
}

input ClienteInput {
    id:ID
    nome:String!
    email:String!
}

extend type Query {
    cliente(id:ID!):Cliente
    clientes:[Cliente]!
}

extend type Mutation {
    saveCliente(cliente: ClienteInput!):Cliente
    deleteCliente(id:ID!):Boolean!
}